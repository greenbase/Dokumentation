@thesis{Holzel.2019,
 author = {Hölzel, Nico},
 year = {2018},
 title = {Ein Bewertungsansatz zur Analyse von Zustandsmanagementsystemen in Verkehrsflugzeugen unter Berücksichtigung neuer Instandhaltungskonzepte},
 abstract = {Diese Forschungsarbeit beschäftigt sich mit der Technologiebewertung von Zustandsmanagementsystemen und zustandsorientierten/prädiktiven Instandhaltungs-konzepten für zukünftige Verkehrsflugzeuge. Dazu wird ein Modell zur Diskret-Ereignis-Simulation des Flugzeugbetriebs, der Ausfall- und Instandhaltungsereignisse und der Wirkung von Diagnose- und Prognosetechnologien im Flugzeuglebenszyklus unter Einbindung eines Optimierungsansatzes für die dynamische Instandhaltungsplanung entwickelt. Für die Bewertung des ökonomischen Nutzens aus Betreibersicht kommt eine Kosten-Nutzen-Analyse auf Basis der Discounted Cash-Flow Methode zum Einsatz. In einem Anwendungsfall wird das operationelle und wirtschaftliche Potenzial von Zustands-managementsystemen am Beispiel eines Kurz-/Mittelstreckenflugzeugs aufgezeigt.

This research study deals with the technology assessment of health management systems and condition-based/predictive maintenance concepts for future commercial aircraft. A model for a discrete-event-simulation of aircraft operation, failure and maintenance events and the impacts of diagnostic and prognostic technologies in the aircraft life cycle with the integration of an optimization approach for the dynamic maintenance planning is developed. For the evaluation of the economic benefit from an operator's perspective, a cost-benefit analysis based on the discounted cash flow method is applied. In an application case, the operational and economic potential of health management systems is demonstrated by the example of a short/medium-haul aircraft.},
 institution = {DLR},
